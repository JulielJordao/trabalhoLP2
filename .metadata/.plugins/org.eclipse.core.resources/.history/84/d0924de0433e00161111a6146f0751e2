package entidade;

public class Promocao {
	private int cod_promocao;
	private int val_desconto;
	
	public int getCod_promocao() {
		return cod_promocao;
	}
	public void setCod_promocao(int cod_promocao) {
		this.cod_promocao = cod_promocao;
	}
	public int getVal_desconto() {
		return val_desconto;
	}
	public void setVal_desconto(int val_desconto) {
		this.val_desconto = val_desconto;
	}
}
